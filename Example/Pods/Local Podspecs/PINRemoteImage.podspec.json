{
  "name": "PINRemoteImage",
  "version": "1.2",
  "summary": "A thread safe, performant, feature rich image fetcher",
  "homepage": "https://github.com/pinterest/PINRemoteImage",
  "license": "Apache 2.0",
  "authors": {
    "Garrett Moon": "garrett@pinterest.com"
  },
  "source": {
    "git": "https://github.com/pinterest/PINRemoteImage.git",
    "tag": "1.1.2"
  },
  "social_media_url": "https://twitter.com/garrettmoon",
  "platforms": {
    "ios": "6.0"
  },
  "requires_arc": true,
  "subspecs": [
    {
      "name": "Core",
      "source_files": "Pod/Core/Classes/**/*.{h,m}",
      "public_header_files": "Pod/Core/Classes/**/*.h",
      "frameworks": [
        "UIKit",
        "ImageIO",
        "CoreImage"
      ],
      "dependencies": {
        "PINCache": [
          ">=2.0"
        ]
      }
    },
    {
      "name": "FLAnimatedImage",
      "source_files": "Pod/FLAnimatedImage/Classes/**/*.{h,m}",
      "public_header_files": "Pod/FLAnimatedImage/Classes/**/*.h",
      "dependencies": {
        "PINRemoteImage/Core": [
          "1.2"
        ],
        "FLAnimatedImage": [
          ">= 1.0"
        ]
      }
    }
  ]
}
